<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:focusable="true"

    android:focusableInTouchMode="true">

    <!-- TODO: Update blank fragment layout -->

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/rel_data"
        android:clickable="true">


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rel_data"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="100dp"
        android:clickable="true"
        android:orientation="vertical"
        android:visibility="visible">


        <TextView
            android:id="@+id/selectyear"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="5dp"
            android:layout_marginRight="40dp"
            android:text="Select Year" />

        <com.tdms.mahyco.nxg.utils.MultiSelectionSpinner
            android:id="@+id/ddlYear"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_below="@+id/selectyear"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="3dp"
            android:layout_marginRight="40dp"
            android:background="@drawable/spinner_background" />


        <TextView
            android:id="@+id/selectseason"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ddlYear"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="5dp"
            android:layout_marginRight="40dp"
            android:text="Select Season"

            />

        <com.tdms.mahyco.nxg.utils.MultiSelectionSpinner
            android:id="@+id/ddlSesion"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_below="@+id/selectseason"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="3dp"
            android:layout_marginRight="40dp"
            android:background="@drawable/spinner_background" />


        <TextView
            android:id="@+id/code"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ddlSesion"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="5dp"
            android:layout_marginRight="40dp"
            android:text="Enter Your Code"

            />

        <EditText
            android:id="@+id/txtStaff"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/code"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="3dp"
            android:layout_marginRight="40dp"
            android:background="@drawable/border_style"
            android:drawableLeft="@drawable/user"
            android:ems="10"
            android:hint="Enter Your Code"
            android:text=""

            android:textColorLink="@android:color/black" />

        <EditText
            android:id="@+id/txtTFA"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txtStaff"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="40dp"
            android:background="@drawable/border_style"
            android:drawableLeft="@drawable/user"
            android:ems="10"
            android:hint="TFA Code"

            android:text=""
            android:textColorLink="@android:color/black"
            android:visibility="gone" />

        <Spinner
            android:id="@+id/ddlStage"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="40dp"
            android:background="@drawable/spinner_background"
            android:visibility="gone" />

        <Spinner
            android:id="@+id/ddlTrailCode"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="40dp"
            android:background="@drawable/spinner_background"
            android:visibility="gone" />

        <Button
            android:id="@+id/btnDownloadData"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_below="@+id/txtStaff"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="15dp"
            android:background="@drawable/buttonbordet"
            android:drawableLeft="@drawable/downlaod"
            android:gravity="center"
            android:text="Download Data" />

        <Button
            android:id="@+id/btnDownloadDataLocation"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_below="@id/btnDownloadData"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="15dp"
            android:background="@drawable/buttonbordet"
            android:drawableLeft="@drawable/downlaod"
            android:gravity="center"
            android:text="Download Location Data" />

        <Button
            android:id="@+id/btnDownloadFeedbackData"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_below="@+id/btnDownloadData"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="35dp"
            android:background="@drawable/buttonbordet"
            android:drawableLeft="@drawable/downlaod"
            android:gravity="center"
            android:text="Download Feedback Data"
            android:visibility="invisible" />

        <!--<ProgressBar-->
        <!--android:id="@+id/progressBar2"-->
        <!--style="?android:attr/progressBarStyle"-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:indeterminate="true"-->
        <!--android:layout_centerInParent="true"-->
        <!--android:indeterminateTint="@color/colorPrimary"-->
        <!--android:indeterminateTintMode="src_atop"-->
        <!--android:visibility="invisible" />-->


    </RelativeLayout>

</RelativeLayout>